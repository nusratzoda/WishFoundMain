@model List<Domain.Dtos.GetCategoryDto> 
<table class="table">
    <thead>
    <tr>
        <th scope="col">#</th>
        <th scope="col">Name</th>
        <th scope="col"></th>
        
    </tr>
    </thead>
    <tbody>
    
    @foreach(var category in Model)
    {
        <tr>
            <th scope="row">@category.Id</th>
            <td>@category.Name</td>
            <td> <a href="/admin/category/edit/@category.Id">Edit</a> | <a href="/admin/category/delete/@category.Id">Delete</a></td>
        </tr>
    }
   
    </tbody>
</table>